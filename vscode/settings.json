{
	"editor.accessibilitySupport": "off",
	"editor.autoClosingQuotes": "beforeWhitespace",
	"editor.bracketPairColorization.enabled": true,
	"editor.dragAndDrop": false,
	"editor.emptySelectionClipboard": false,
	"editor.fontFamily": "'SF Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.guides.indentation": false,
	"editor.inlineSuggest.enabled": true,
	"editor.minimap.enabled": false,
	"editor.renderControlCharacters": true,
	"editor.renderWhitespace": "boundary",
	"editor.rulers": [
		79,
		99
	],
	"editor.scrollbar.horizontal": "visible",
	"editor.semanticHighlighting.enabled": true,
	"editor.suggestSelection": "first",
	"editor.tabSize": 8,
	"editor.wordSeparators": " !\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~",
	"editor.wordWrapColumn": 99,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.openEditors.sortOrder": "alphabetical",
	"extensions.ignoreRecommendations": true,
	"files.associations": {
		"*.tcc": "c",
		"*.awk": "makefile",
		"*.in": "makefile",
		"*.mdd": "makefile",
		"Make*": "makefile",
		"*.ztst": "plaintext",
		"_*": "shellscript",
		".*": "shellscript",
	},
	"files.autoSave": "afterDelay",
	"files.defaultLanguage": "shellscript",
	"files.exclude": {
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/*.zwc": true
	},
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"git.confirmSync": false,
	"githubIssues.queries": [
		{
			"label": "Open Issues",
			"query": "state:open repo:${owner}/${repository} sort:created-desc"
		}
	],
	"highlight.regexes": {
		"(private)": [
			{
				"color": "#569CD6"
			}
		]
	},
	"java.autobuild.enabled": false,
	"java.semanticHighlighting.enabled": true,
	"reflowMarkdown.doubleSpaceBetweenSentences": true,
	"reflowMarkdown.preferredLineLength": 99,
	"scm.inputFontFamily": "editor",
	"scm.inputFontSize": 11,
	"search.sortOrder": "fileNames",
	"search.useReplacePreview": false,
	"shebang.associations": [
		{
			"pattern": "^#!/bin/*$",
			"language": "shellscript"
		},
		{
			"pattern": "^#autoload$",
			"language": "shellscript"
		},
		{
			"pattern": "^#compdef *$",
			"language": "shellscript"
		},
	],
	"shell-syntax.defaultShell": "zsh",
	"terminal.external.linuxExec": "konsole",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"workbench.colorCustomizations": {
		"[*]": {
			"editor.background": "#0000001e",
			"editor.findMatchBackground": "#0000",
			"editor.findMatchBorder": "#eaea0080",
			"editor.findMatchHighlightBackground": "#0000",
			"editor.findMatchHighlightBorder": "#eaea0055",
			"editor.inactiveSelectionBackground": "#36363680",
			"editor.selectionBackground": "#314f7880",
			"editor.selectionHighlightBackground": "#0000",
			"editor.selectionHighlightBorder": "#ffffff55",
			"editorWhitespace.foreground": "#e3e4e280",
			"sideBar.background": "#0000001e",
		}
	},
	"workbench.editor.enablePreview": false,
	"workbench.editor.limit.enabled": true,
	"workbench.editor.showTabs": false,
	"workbench.editor.untitled.hint": "hidden",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"workbench.fontAliasing": "antialiased",
	"workbench.list.keyboardNavigation": "filter",
	"workbench.startupEditor": "none",
	"diffEditor.renderSideBySide": false,
	"editor.renderLineHighlight": "all",
}
