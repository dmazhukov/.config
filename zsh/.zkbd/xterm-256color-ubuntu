#!/bin/zsh
typeset -gA key=(
    Control-Space     $'\0'
    Tab               $'\t'
    Shift-Tab         $'\e[Z'
    Return            $'\r'
    Backspace         $'\C-?'
    Control-Backspace $'\C-H'
    Delete            $'\e[3~'
    Control-Delete    $'\e[3;5~'
    Up                $'\e[A'
    Alt-Up            $'\e[1;3A'
    Down              $'\e[B'
    Alt-Down          $'\e[1;3B'
    Left              $'\e[D'
    Control-Left      $'\e[1;5D'
    Right             $'\e[C'
    Control-Right     $'\e[1;5C'
    PageUp            $'\e[5~'
    PageDown          $'\e[6~'
    Home              $'\e[H'
    Control-Home      $'\e[1;5H'
    End               $'\e[F'
    Control-End       $'\e[1;5F'
)
